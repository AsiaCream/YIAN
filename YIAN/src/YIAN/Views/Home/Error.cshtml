<div id="page-wrapper">
    <div class="row">
        <div class="col-md-12">
            <p class="text-center" style="font-size:40px;">无法找到请求</p>
            <p id="timeTxt" class="text-center" style="font-size:40px;"></p>
        </div>
    </div>
</div>
<script type="text/javascript">
    var timer = null;
    var num = 3;
    timer = setInterval(function () {

        if (num == 0) {
            clearInterval(timer);
            window.history.go(-1);
        }
        document.getElementById("timeTxt").innerHTML = num + "秒后自动跳转回上一页...";
        num--;
    }, 1000);


</script>
